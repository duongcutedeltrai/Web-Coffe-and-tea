{
    "version": "1.0.0",
    "configurations": [
        {
            "name": "debug-pblCoffe",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "start:debug"
            ],
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ]
        }
    ]
}